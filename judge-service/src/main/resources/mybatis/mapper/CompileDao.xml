<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.cloudli.judgeservice.dao.CompileDao">
    <select id="get" parameterType="top.cloudli.judgeservice.model.Compile">
        select *
        from compile
        where solution_id = #{solutionId};
    </select>

    <insert id="add" parameterType="top.cloudli.judgeservice.model.Compile">
        insert into compile (solution_id, state, info)
        values (#{solutionId}, #{state}, #{info});
    </insert>
</mapper>