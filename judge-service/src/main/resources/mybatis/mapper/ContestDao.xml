<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.cloudli.judgeservice.dao.ContestDao">
    <select id="getContest" resultType="top.cloudli.judgeservice.model.Contest">
        select contest_name, end_at, languages
        from contest
        where contest_id = #{contestId};
    </select>
</mapper>